using System;

class Program
{
    public static void Main(string[] args)
    {
        Console.WriteLine("Por favor usu√°rio digite o tamanho de sua sequencia de Fibonacci");
        int num0 = 0;
        int num1 = 1;
        int tamanho = int.Parse(Console.ReadLine());

        for (int i = 0; i < tamanho; i++)
        {
            Console.WriteLine(num0);
            num1 = num0 + num1;
            num0 = num1 - num0;

        }
    }
}
